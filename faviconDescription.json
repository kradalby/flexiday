
{
	"masterPicture": "src/assets/img/flexiday_plane_white.svg",
	"iconsPath": "/favicons",
	"design": {
		"ios": {
			"pictureAspect": "backgroundAndMargin",
			"backgroundColor": "#343a40",
			"margin": "35%",
			"assets": {
				"ios6AndPriorIcons": true,
				"ios7AndLaterIcons": true,
				"precomposedIcons": true,
				"declareOnlyDefaultIcon": true
			},
			"appName": "Flexiday"
		},
		"desktopBrowser": {},
		"windows": {
			"pictureAspect": "noChange",
			"backgroundColor": "#343a40",
			"onConflict": "override",
			"assets": {
				"windows80Ie10Tile": true,
				"windows10Ie11EdgeTiles": {
					"small": true,
					"medium": true,
					"big": true,
					"rectangle": true
				}
			},
			"appName": "Flexiday"
		},
		"androidChrome": {
			"pictureAspect": "backgroundAndMargin",
			"margin": "21%",
			"backgroundColor": "#343a40",
			"themeColor": "#343a40",
			"manifest": {
				"name": "Flexiday",
				"display": "standalone",
				"orientation": "notSet",
				"onConflict": "override",
				"declared": true
			},
			"assets": {
				"legacyIcon": false,
				"lowResolutionIcons": false
			}
		},
		"safariPinnedTab": {
			"pictureAspect": "silhouette",
			"themeColor": "#5bbad5"
		}
	},
	"settings": {
		"scalingAlgorithm": "Mitchell",
		"errorOnImageTooSmall": false
	}
}
